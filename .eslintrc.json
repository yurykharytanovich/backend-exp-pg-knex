{
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": "eslint:recommended",
  "rules": {
    "semi": ["error"],
    "indent": ["error", 4],
    "no-console": "off",
    "comma-dangle": ["error", "always-multiline"]
  }
}