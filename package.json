{
  "name": "backend-prototype",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --watch ./ --exec babel-node ./app/server.js --inspect",
    "fill-mock": "./node_modules/.bin/babel-node app/mock-data/fill.js",
    "clear-mock": "./node_modules/.bin/babel-node app/mock-data/clear.js",
    "refill-mock": "npm run clear-mock && npm run fill-mock",
    "up": "knex-migrate up",
    "down": "knex-migrate down",
    "rollback": "knex-migrate rollback",
    "down-all": "knex-migrate down --to 0",
    "migration": "knex migrate:make",
    "remigrate": "npm run down-all && npm run up",
    "test": "cross-env NODE_ENV=test mocha ./app/**/*.test.js ./app/**/**/*.test.js --compilers js:babel-core/register --require babel-polyfill || exit 0",
    "test-pre-commit": "cross-env NODE_ENV=test mocha ./app/**/*.test.js ./app/**/**/*.test.js --compilers js:babel-core/register --require babel-polyfill",
    "eslint": "./node_modules/.bin/eslint --ext .js --color ./ || exit 0",
    "eslint-pre-commit": "./node_modules/.bin/eslint --ext .js --color ./"
  },
  "pre-commit": [
    "test-pre-commit",
    "eslint-pre-commit"
  ],
  "dependencies": {
    "and": "^0.0.0",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.1",
    "cors": "^2.8.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "jsonwebtoken": "^7.3.0",
    "knex": "^0.13.0",
    "knex-migrate": "^1.3.0",
    "lodash": "^4.17.4",
    "morgan": "^1.8.1",
    "pg-promise": "^5.6.4",
    "squel": "^5.11.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "cross-env": "^4.0.0",
    "csvtojson": "^1.1.7",
    "db-migrate": "^0.10.0-beta.24",
    "db-migrate-pg": "^0.1.11",
    "eslint": "^4.9.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.1",
    "uuid": "^3.1.0"
  },
  "author": "",
  "license": "ISC"
}
